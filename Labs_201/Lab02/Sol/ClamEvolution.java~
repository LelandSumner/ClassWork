public class Clam {

    public static void main (String [] args) {
        printClam();
    }

    public static void printClam() {
        System.out.println("  -------");
        System.out.println(" /       \\");
        System.out.println("/         \\");
        System.out.println("-\"-'-\"-'-\"-");
        System.out.println("\\         /");
        System.out.println(" \\       /");
        System.out.println("  -------");
    }

} // end class
